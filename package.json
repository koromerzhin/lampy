{
  "name": "@koromerzhin/lampy",
  "version": "1.0.0",
  "description": "Example local server",
  "scripts": {
    "docker_swarm-init": "korojscommands docker_swarm-init",
    "docker_create-network": "korojscommands docker_create-network",
    "docker_getpull-image": "korojscommands docker_getpull-image",
    "docker_deploy": "korojscommands docker_deploy --files docker-compose.yml",
    "waiting:all": "korojscommands docker_waiting --container mercure redis mariadb postgresql mailhog phpmyadmin portainer traefik --status running",
    "docker_ls": "korojscommands docker_ls",
    "exec": "npm run docker_swarm-init && npm run docker_create-network && npm run docker_getpull-image && npm run docker_deploy && npm run waiting:all && npm run docker_ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koromerzhin/lampy.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koromerzhin/lampy/issues"
  },
  "homepage": "https://github.com/koromerzhin/lampy#readme",
  "dependencies": {
    "korojscommands": "^1.1.3"
  }
}
